class Comlinetest
{
    public static void main (String args[])
    {
        int count, i=0;
        String s;
        count=args.length;
        System.out.println("Number of arguments are " + count);
        while(i<count)
        {
            s=args[i];
            i++;
            System.out.println("Java is " + s);
        }
    }
}
